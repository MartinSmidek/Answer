    panel men {title:"Menu", include:'onclick'}
    panel err {title:"Error", include:'onclick'}
  }
  tabs sys { title:'Systém' include:'onclick,libr.syst' }
  tabs hlp {title:"Nápověda",  include:'onclick,libr.help' }
  tabs fce {title:"Balíky",  include:'onclick' }
