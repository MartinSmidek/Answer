menu test { type:'main'
  var global: text,
  tabs elem {title:"Testy",
    panel grp {title:"Group", include:'onclick'
      menu {type:'left',
        menu {title:'Příklady objednávek',type:'group'
          item {title:'711'                   ,par:°{no:'711'}}
          item {title:'771'                   ,par:°{no:'771'}}
        }
        proc onclick (i) { the_order.set(i.par.no); show_order(i.par.no) }
