// testy funkcí
panel inc { title:'Include' include:'onclick,libr.syst' }
panel m { title:'Menu'
  menu {type:'left',
    menu {title:'funkce',type:'group'
      item {title:'test 1' }
      item {title:'test 2' }
    }
    menu {title:'výrazy',type:'group'
      item {title:'test return'
        proc onclick() { echo(ahoj) }
        proc ahoj () { return('ahoj') }
      }
      item {title:'vnořená volání'
        proc onclick() { echo(p1(1,10)) }
        proc p1 (x,y) { p2(sum(x,1),20) }
        proc p2 (x,y) { p3(sum(x,1),30) }
        proc p3 (x,y) { return(sum(x,1)) }
      }
    }
    proc onclick(i) { u.result.set(i.title) }
  }
  use u: form f [0,0,,]
}
